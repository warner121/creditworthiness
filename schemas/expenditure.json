{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "required": [
      "retirementStatus",
      "numberOfAdults",
      "numberOfChildren"
    ],
    "properties": {
      "retirementStatus": {
        "type": "string",
        "enum": [
          "state_pension",
          "other_retired",
          "non_retired"
        ]
      },
      "numberOfAdults": {
        "type": "integer",
        "minimum": 0,
        "maximum": 9
      },
      "numberOfChildren": {
        "type": "integer",
        "minimum": 0,
        "maximum": 9
      },
      "alcoholicDrinksTobaccoAndNarcotics": {
        "type": "number",
        "minimum": 0,
        "maximum": 999
      },
      "clothingAndFootwear": {
        "type": "number",
        "minimum": 0,
        "maximum": 999
      },
      "communication": {
        "type": "number",
        "minimum": 0,
        "maximum": 999
      },
      "education": {
        "type": "number",
        "minimum": 0,
        "maximum": 999
      },
      "foodAndNonAlcoholicDrinks": {
        "type": "number",
        "minimum": 0,
        "maximum": 999
      },
      "health": {
        "type": "number",
        "minimum": 0,
        "maximum": 999
      },
      "householdGoodsAndServices": {
        "type": "number",
        "minimum": 0,
        "maximum": 999
      },
      "miscellaneousGoodsAndServices": {
        "type": "number",
        "minimum": 0,
        "maximum": 999
      },
      "netHousingFuelAndPower": {
        "type": "number",
        "minimum": 0,
        "maximum": 999
      },
      "otherExpenditureItems": {
        "type": "number",
        "minimum": 0,
        "maximum": 999
      },
      "recreationAndCulture": {
        "type": "number",
        "minimum": 0,
        "maximum": 999
      },
      "restaurantsAndHotels": {
        "type": "number",
        "minimum": 0,
        "maximum": 999
      },
      "transport": {
        "type": "number",
        "minimum": 0,
        "maximum": 999
      }
    }
  },
  "minItems": 1
}