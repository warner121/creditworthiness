{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "required": [
      "retirementStatus",
      "numberOfAdults",
      "numberOfChildren"
    ],
    "properties": {
      "retirementStatus": {
        "type": "string",
        "enum": [
          "state_pension",
          "other_retired",
          "non_retired"
        ]
      },
      "numberOfAdults": {
        "type": "integer",
        "minimum": 0,
        "maximum": 9
      },
      "numberOfChildren": {
        "type": "integer",
        "minimum": 0,
        "maximum": 9
      }
    }
  },
  "minItems": 1
}