{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "required": [
      "monthly_income",
      "mortgage_or_rent",
      "monthly_credit_commitments",
      "employment_status",
      "no_of_dependants",
      "no_of_adults",
      "Age",
      "Sex",
      "Job",
      "Housing",
      "Saving accounts",
      "Checking account",
      "Purpose"
    ],
    "properties": {
      "monthly_income": {
        "type": "number",
        "minimum": 0,
        "maximum": 5000
      },
      "mortgage_or_rent": {
        "type": "number",
        "minimum": 0,
        "maximum": 5000
      },
      "monthly_credit_commitments": {
        "type": "number",
        "minimum": 0,
        "maximum": 5000
      },
      "employment_status": {
        "type": "string",
        "enum": [
          "full_time",
          "part_time",
          "self_employed",
          "student",
          "pension",
          "temporary",
          "benfits"
        ]
      },
      "no_of_dependants": {
        "type": "integer",
        "minimum": 0,
        "maximum": 9
      },
      "no_of_adults": {
        "type": "integer",
        "minimum": 1,
        "maximum": 9
      },
      "Age": {
        "type": "integer",
        "minimum": 19,
        "maximum": 75
      },
      "Sex": {
        "type": "string",
        "enum": [
          "male",
          "female"
        ]
      },
      "Job": {
        "type": "integer",
        "minimum": 0,
        "maximum": 3
      },
      "Housing": {
        "type": "string",
        "enum": [
          "own",
          "rent",
          "free"
        ]
      },
      "Saving accounts": {
        "type": [
          "string",
          "null"
        ],
        "enum": [
          "little",
          "moderate",
          "rich",
          null
        ]
      },
      "Checking account": {
        "type": [
          "string",
          "null"
        ],
        "enum": [
          "little",
          "moderate",
          "rich",
          null
        ]
      },
      "Credit amount": {
        "type": "integer",
        "minimum": 250,
        "maximum": 18400
      },
      "Duration": {
        "type": "integer",
        "minimum": 4,
        "maximum": 72
      },
      "Purpose": {
        "type": "string",
        "enum": [
          "business",
          "car",
          "domestic appliances",
          "education",
          "furniture/equipment",
          "radio/TV",
          "repairs",
          "vacation/others"
        ]
      }
    }
  },
  "minItems": 1
}