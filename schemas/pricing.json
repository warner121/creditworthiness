{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "required": [
      "monthly_income",
      "mortgage_or_rent",
      "monthly_credit_commitments",
      "employment_status",
      "no_of_dependants",
      "no_of_adults",
      "durationInMonths",
      "creditHistory",
      "purpose",
      "creditAmount",
      "installmentRateInPercentageOfDisposableIncome",
      "personalStatusAndSex",
      "otherDebtorsOrGuarantors",
      "presentResidenceSince",
      "property",
      "ageInYears",
      "otherInstallmentPlans",
      "housing",
      "numberOfExistingCreditsAtThisBank",
      "job",
      "numberOfPeopleBeingLiableToProvideMaintenanceFor",
      "telephone",
      "foreignWorker"
    ],
    "properties": {
      "monthly_income": {
        "type": "number",
        "minimum": 0,
        "maximum": 5000
      },
      "mortgage_or_rent": {
        "type": "number",
        "minimum": 0,
        "maximum": 5000
      },
      "monthly_credit_commitments": {
        "type": "number",
        "minimum": 0,
        "maximum": 5000
      },
      "employment_status": {
        "type": "string",
        "enum": [
          "full_time",
          "part_time",
          "self_employed",
          "student",
          "pension",
          "temporary",
          "benfits"
        ]
      },
      "no_of_dependants": {
        "type": "integer",
        "minimum": 0,
        "maximum": 9
      },
      "no_of_adults": {
        "type": "integer",
        "minimum": 1,
        "maximum": 9
      },
      "statusOfExistingCheckingAccount": {
        "anyOf": [
          {
            "type": "integer",
            "minimum": -99999,
            "maximum": 99999
          },
          {
            "type": "null"
          }
        ]
      },
      "durationInMonths": {
        "type": "integer",
        "minimum": 4,
        "maximum": 72
      },
      "creditHistory": {
        "type": "string",
        "enum": [
          "no credits taken",
          "all credits paid back duly",
          "all credits at this bank paid back duly",
          "existing credits paid back duly till now",
          "delay in paying off in the past",
          "critical account",
          "other credits existing (not at this bank)"
        ]
      },
      "purpose": {
        "type": "string",
        "enum": [
          "car (new)",
          "car (used)",
          "furniture",
          "equipment",
          "radio",
          "television",
          "domesticappliances",
          "repairs",
          "education",
          "vacation",
          "retraining",
          "business",
          "others"
        ]
      },
      "creditAmount": {
        "type": "integer",
        "minimum": 250,
        "maximum": 18400
      },
      "savingsAccountOrBonds": {
        "anyOf": [
          {
            "type": "integer",
            "minimum": 0,
            "maximum": 99999
          },
          {
            "type": "null"
          }
        ]
      },
      "presentEmploymentSince": {
        "anyOf": [
          {
            "type": "integer",
            "minimum": 0,
            "maximum": 99
          },
          {
            "type": "null"
          }
        ]
      },
      "installmentRateInPercentageOfDisposableIncome": {
        "type": "integer",
        "minimum": 1,
        "maximum": 4
      },
      "personalStatusAndSex": {
        "type": "string",
        "enum": [
          "male: divorced",
          "male: separated",
          "female: divorced",
          "female: separated",
          "female: married",
          "male: single",
          "male: married",
          "male: widowed",
          "female: single"
        ]
      },
      "otherDebtorsOrGuarantors": {
        "type": "string",
        "enum": [
          "none",
          "co-applicant",
          "guarantor"
        ]
      },
      "presentResidenceSince": {
        "type": "integer",
        "minimum": 1,
        "maximum": 4
      },
      "property": {
        "type": "string",
        "enum": [
          "real estate",
          "building society savings agreement",
          "life insurance",
          "car",
          "other",
          "unknown",
          "no property"
        ]
      },
      "ageInYears": {
        "type": "integer",
        "minimum": 19,
        "maximum": 75
      },
      "otherInstallmentPlans": {
        "type": "string",
        "enum": [
          "bank",
          "stores",
          "none"
        ]
      },
      "housing": {
        "type": "string",
        "enum": [
          "rent",
          "own",
          "for free"
        ]
      },
      "numberOfExistingCreditsAtThisBank": {
        "type": "integer",
        "minimum": 1,
        "maximum": 4
      },
      "job": {
        "type": "string",
        "enum": [
          "unemployed",
          "unskilled - non-resident",
          "unskilled - resident",
          "skilled employee",
          "official",
          "management",
          "self-employed",
          "highly qualified employee",
          "officer"
        ]
      },
      "numberOfPeopleBeingLiableToProvideMaintenanceFor": {
        "type": "integer",
        "minimum": 1,
        "maximum": 2
      },
      "telephone": {
        "type": "string",
        "enum": [
          "none",
          "yes, registered under the customers name"
        ]
      },
      "foreignWorker": {
        "type": "string",
        "enum": [
          "yes",
          "no"
        ]
      }
    }
  },
  "minItems": 1
}